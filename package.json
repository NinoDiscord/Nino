{
  "name": "nino",
  "description": "Moderation bot for Discord's hack week entry.",
  "version": "0.7.0",
  "main": "dist/index.js",
  "scripts": {
    "build": "yarn lint && yarn test && yarn clean && tsc",
    "build:watch": "yarn lint && yarn test && yarn clean && tsc -w",
    "main": "yarn build && node dist/index.js",
    "dev": "nodemon dist/index.js",
    "lint": "eslint src --ext .ts --fix",
    "clean": "rm -fr dist",
    "test": "jest --config jest.config.js --forceExit"
  },
  "maintainers": [
    "Noel (nowoel)",
    "dondish"
  ],
  "contributors": [
    "Kyle (dvhe)",
    "Wesselgame",
    "ohlookitsderpy"
  ],
  "dependencies": {
    "@augu/immutable": "0.1.6",
    "@sentry/node": "5.12.0",
    "common-tags": "1.8.0",
    "eris": "0.11.1",
    "inversify": "5.0.1",
    "inversify-inject-decorators": "3.1.0",
    "ioredis": "4.14.1",
    "js-yaml": "3.13.1",
    "leeks.js": "0.0.8",
    "mongoose": "5.8.11",
    "ms": "2.1.2",
    "prom-client": "11.5.3",
    "reflect-metadata": "0.1.13",
    "wumpfetch": "0.3.1"
  },
  "devDependencies": {
    "@types/common-tags": "1.8.0",
    "@types/express": "4.17.2",
    "@types/ioredis": "4.0.20",
    "@types/jest": "25.1.1",
    "@types/js-yaml": "3.12.2",
    "@types/mongoose": "5.7.0",
    "@types/ms": "0.7.31",
    "@types/node": "12.12.21",
    "@typescript-eslint/eslint-plugin": "2.19.0",
    "@typescript-eslint/parser": "2.19.0",
    "eslint": "6.8.0",
    "jest": "25.1.0",
    "jest-mock-extended": "1.0.8",
    "nodemon": "2.0.2",
    "ts-jest": "25.2.0",
    "typescript": "3.7.5"
  }
}