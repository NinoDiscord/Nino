{
    "name": "nino",
    "description": "Moderation bot for Discord's hack week entry.",
    "version": "0.6.1",
    "main": "dist/index.js",
    "scripts": {
        "build": "yarn clean && tsc",
        "build:watch": "yarn clean && tsc -w",
        "main": "yarn build && node dist/index.js",
        "dev": "nodemon dist/index.js",
        "lint": "prettier --write --single-quote --trailing-comma es5 --parser typescript \"src/**/*.ts\"",
        "clean": "rm -fr dist",
        "test": "jest"
    },
    "maintainers": [
        "August (auguwu)",
        "dondish"
    ],
    "contributors": [
        "Kyle (dvhe)",
        "Wesselgame",
        "ohlookitsderpy"
    ],
    "dependencies": {
        "@augu/immutable": "0.1.0",
        "@sentry/integrations": "5.10.2",
        "@sentry/node": "5.10.2",
        "common-tags": "1.8.0",
        "eris": "0.11.1",
        "express": "4.17.1",
        "inversify": "^5.0.1",
        "inversify-inject-decorators": "^3.1.0",
        "ioredis": "4.14.1",
        "js-yaml": "3.13.1",
        "mongoose": "5.8.2",
        "ms": "2.1.2",
        "pikmin": "0.1.13",
        "prom-client": "11.5.3",
        "reflect-metadata": "^0.1.13",
        "wumpfetch": "0.3.1"
    },
    "devDependencies": {
        "@types/common-tags": "1.8.0",
        "@types/express": "4.17.2",
        "@types/ioredis": "4.0.20",
        "@types/jest": "24.0.24",
        "@types/js-yaml": "3.12.1",
        "@types/mongoose": "5.5.34",
        "@types/ms": "0.7.31",
        "@types/node": "12.12.21",
        "@typescript-eslint/eslint-plugin": "2.12.0",
        "@typescript-eslint/parser": "2.12.0",
        "eslint": "6.8.0",
        "jest": "24.9.0",
        "nodemon": "2.0.2",
        "prettier": "1.19.1",
        "ts-jest": "24.2.0",
        "typescript": "3.7.3"
    },
    "jest": {
        "transform": {
            "^.+\\.tsx?$": "ts-jest"
        },
        "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json"
        ]
    }
}
