{
  "name": "@august/nino",
  "description": "ðŸ”¨ Advanced and cute moderation discord bot as an entry of Discord's Hack Week",
  "version": "1.0.0-indev.0",
  "private": true,
  "homepage": "https://nino.floofy.dev",
  "license": "MIT",
  "repository": "https://github.com/NinoDiscord/Nino",
  "main": "build/main.js",
  "author": "August <cutie@floofy.dev>",
  "maintainers": [
    {
      "email": "cutie@floofy.dev",
      "name": "August",
      "url": "https://floofy.dev"
    }
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "docker:build:canary": "docker buuild . -t auguwu/nino:canary",
    "clean:node_modules": "rm -rf node_modules/**/node_modules && rm -rf node_modules/@types/**/node_modules && rm -rf node_modules/@augu/**/node_modules",
    "clean:win:tar": "cp node_modules/@augu/collections/build/index.js.* node_modules/@augu/collections/build/index.js && rm node_modules/@augu/collections/build/index.js.*",
    "docker:build": "docker build . -t auguwu/nino:latest",
    "prepare": "npm run clean:node_modules",
    "sync:db": "npm run build && typeorm schema:sync",
    "build": "npm run lint && rm -rf build && tsc",
    "start": "node build/main.js",
    "lint": "eslint src --ext .ts --fix"
  },
  "dependencies": {
    "@augu/collections": "1.0.5",
    "@augu/dotenv": "1.3.0",
    "@augu/lilith": "1.1.3",
    "@augu/orchid": "2.2.3",
    "@augu/utils": "1.2.0",
    "@kubernetes/client-node": "0.14.0",
    "@sentry/node": "6.2.2",
    "eris": "github:abalabahaha/eris#dev",
    "express": "4.17.1",
    "ioredis": "4.24.2",
    "js-yaml": "4.0.0",
    "ms": "2.1.2",
    "pg": "8.5.1",
    "reflect-metadata": "0.1.13",
    "source-map-support": "0.5.19",
    "tslog": "3.1.2",
    "typeorm": "0.2.31"
  },
  "devDependencies": {
    "@augu/eslint-config": "1.10.0",
    "@augu/tsconfig": "1.0.1",
    "@types/express": "4.17.11",
    "@types/ioredis": "4.22.0",
    "@types/js-yaml": "4.0.0",
    "@types/node": "14.14.35",
    "@typescript-eslint/eslint-plugin": "4.18.0",
    "@typescript-eslint/parser": "4.18.0",
    "discord-api-types": "0.12.1",
    "eslint": "7.22.0",
    "ts-node": "9.1.1",
    "typescript": "4.2.3"
  }
}
